  "data_structure_notes": {
    "event_format": {
      "id": "Unique event identifier (number)",
      "timestamp": "ISO 8601 timestamp string (when event started)",
      "duration": "Duration in seconds (float)",
      "data": "Event-specific data (object with varying fields)"
    },
    "common_data_fields": {
      "window_events": {
        "app": "Application name (string)",
        "title": "Window title (string)",
        "url": "URL if applicable (string or null)",
        "incognito": "Whether in incognito/private mode (boolean)"
      },
      "afk_events": {
        "status": "Either 'afk' or 'not-afk' (string)"
      },
      "web_events": {
        "url": "Current webpage URL (string)",
        "title": "Page title (string)",
        "audible": "Whether page is playing audio (boolean)",
        "incognito": "Whether in incognito mode (boolean)",
        "tabCount": "Number of open tabs (number)"
      }
    },
    "json_format": "Yes, all ActivityWatch data is in JSON format",
    "api_endpoints": {
      "buckets": "GET /api/0/buckets - List all buckets",
      "events": "GET /api/0/buckets/{bucket_id}/events - Get events from bucket",
      "query": "POST /api/0/query - Run custom queries"
    }
  }
